<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:dc="http://purl.org/dc/elements/1.1/">
	<!-- Author: Christopher P. Barnes -->
	<!-- Adapted from examples at : http://www.linkeddatatools.com/introducing-rdfs-owl -->

	<!-- OWL Header  -->
	<owl:Ontology rdf:about="http://senrabc.github.io/ontologies/eorg/eorg">
		<dc:title>The Exempt Organizations Ontology</dc:title>
		<dc:description>An ontology written for the purpose of classifying exempt organizations that are typically philanthropic</dc:description>
	</owl:Ontology>

	<!-- OWL Class  -->
	<owl:Class rdf:about="http://senrabc.github.io/ontologies/eorg/eorg#exemptOrganization">
		<rdfs:label>exempt organization.</rdfs:label>
		<rdfs:comment>The class that holds the exempt organizations.</rdfs:comment>
	</owl:Class>	

	<owl:DatatypeProperty rdf:ID="exemptOrgName">
		<rdfs:domain rdf:resource="#exemptOrganization"/>
		<rdfs:range rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>

	<!-- These are the object properties of an exempt Organization/ -->
	<owl:ObjectProperty rdf:ID="hasExemptOrgName">
		<rdfs:domain rdf:resource="#domainName
"/>
		<rdfs:range rdf:resource="#registrant"/>
		<owl:inverseOf rdf:resource="#registrantOf"/>
	</owl:ObjectProperty>

	<owl:ObjectProperty rdf:ID="registrantOf">
		<rdfs:domain rdf:resource="#registrant"/>
		<rdfs:range rdf:resource="#domainName"/>
		<owl:inverseOf rdf:resource="#hasRegistrant"/>
	</owl:ObjectProperty>

</rdf:RDF>

